{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/companyNew.schema.json",
  "type": "object",
  "properties": {
    "theme_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "poem_id": {
      "type": "integer"
    },
    "highlighted_lines": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "minItems": 1
    },
    "analysis": {
      "type": "string"
    },
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    }
  },
  "additionalProperties": false,
  "required": [
    "theme_name",
    "poem_id",
    "highlighted_lines",
    "analysis",
    "username"
  ]
}
